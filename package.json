{
  "name": "github-issue-viewer",
  "version": "1.0.0",
  "private": true,
  "config": {
    "buildDir": "./"
  },
  "devDependencies": {
    "grunt": "~0.4.0",
    "grunt-concurrent": "~1.0.0",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-connect": "~0.9.0",
    "grunt-contrib-jasmine": "~0.9.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-watch": "~0.6.0",
    "load-grunt-config": "~0.16.0",
    "jasmine-ajax": "~3.1.0"
  },
  "jshintConfig": {
    "globals": {
      "jasmine": true,
      "describe": true,
      "ddescribe": true,
      "beforeEach": true,
      "afterEach": true,
      "it": true,
      "iit": true,
      "expect": true,
      "inject": true,      
      "process": true,
      "sinon": false,
      "xit": false,
      "xdescribe": false,
      "spyOn": false,
      "React": true,
      "CommentListItem": true,
      "ErrorMessage": true,
      "Issue": true,
      "Comments": true,
      "IssueListItem": true,
      "Issues": true,
      "Label": true,
      "Labels": true,
      "Pagination": true,
      "Spinner": true,
      "UserMeta": true,
      "Utils": true,
      "Main": true,
      "marked": true
