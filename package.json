    "globalstrict": true,
    "devel": true,
    "browser": true,
    "boss": true,
    "eqnull": false,
    "esnext": false,
    "debug": true,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": true,
    "loopfunc": false,
    "multistr": false,
    "proto": false,
    "scripturl": false,
    "smarttabs": false,
    "sub": false,
    "supernew": false,
    "bitwise": false,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonew": false,
    "plusplus": false,
    "undef": true,
    "unused": false,
    "strict": true,
    "white": false,
    "trailing": true,
    "maxparams": false,
    "quotmark": true
  }
}
